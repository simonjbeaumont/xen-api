OCAMLPACKS = xml-light2 uuid rpclib
OCAMLINCLUDES += ..
OCAML_LIBS += ../datamodel

OCamlProgram(main, main)

CAMLP4_FILES = main
UseCamlp4(rpclib.syntax, $(CAMLP4_FILES))

.PHONY: clean
clean:
        rm -f *~ *.opt *.run *.a *.cma *.cmxa *.cmo *.cmi *.o *.cmx *.omc *.annot main *.json

.PHONY: doc-json
doc-json: main
	./main

